{
  "name": "@nejcm/js-library-boilerplate",
  "version": "0.0.0-semantically-released",
  "author": "Nejc Mursic <nmursi2@gmail.com> (https://github.com/nejcm)",
  "description": "Starter kit for your js library projects",
  "source": "src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/inded.esm.js",
  "typings": "typings/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcm/js-library-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/nejc/js-library-boilerplate/issues"
  },
  "homepage": "https://github.com/nejcm/js-library-boilerplate",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "js",
    "es6",
    "react",
    "library"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "format": "prettier ./src/**/*.js --write",
    "clean": "rimraf dist coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "npm run lint && jest",
    "test:cov": "jest --coverage --colors && jest-coverage-badges",
    "test:build": "jest --coverage --colors --silent && jest-coverage-badges",
    "test:watch": "jest --watch",
    "docs:dev": "docz dev",
    "docs:build": "docz build",
    "postdocs:build": "cpy \"**/*\" ../../.docz/dist --cwd=\"other/public\" --parents",
    "docs:serve": "serve ./.docz/dist --listen 6006 --single",
    "build": "npm run clean && rollup -c",
    "validate": "npm run format && npm run lint && npm run test:build",
    "setup": "npm install && npm run validate",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.4",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@semantic-release/npm": "^7.0.4",
    "@storybook/addon-actions": "^5.3.17",
    "storybook-addon-jsx": "^7.1.15",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-notes": "^5.3.17",
    "@storybook/addon-options": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^25.1.4",
    "@types/react": "16.9.23",
    "autoprefixer": "^9.7.4",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-no-side-effect-class-properties": "0.0.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cpy-cli": "^3.1.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "docz": "^2.2.0",
    "docz-theme-default": "^1.2.0",
    "eslint-config-kentcdodds": "^14.8.0",
    "eslint-plugin-react": "7.19.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "0.4.2",
    "lint-staged": "10.0.8",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.27",
    "postcss-loader": "^3.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^3.0.2",
    "rollup": "2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-uglify": "^6.0.4",
    "sass-loader": "^8.0.2",
    "semantic-release": "17.0.4",
    "serve": "^11.3.0",
    "start-server-and-test": "^1.10.11",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3"
  }
}
